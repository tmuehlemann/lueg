{
	"name": "lueg",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"db:generate": "drizzle-kit generate:mysql",
		"db:migrate": "drizzle-kit push:mysql",
		"db:fresh": "drizzle-kit drop",
		"db:check": "drizzle-kit check:mysql",
		"db:studio": "drizzle-kit studio --verbose",
		"db:seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/server/db/seed.ts",
		"metadata:clean": "vite-node --options.transformMode.ssr='/.*/' src/lib/server/metadata/clean.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/tinycolor2": "^1.4.6",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.18",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.57.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.1.5",
		"vite-node": "^1.3.1",
		"vitest": "^1.3.1"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-mysql": "^2.1.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"colorthief": "^2.4.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.29.5",
		"glob": "^10.3.10",
		"lucia": "^2.7.7",
		"lucide-svelte": "^0.302.0",
		"mysql2": "^3.9.2",
		"tailwind-merge": "^2.2.1",
		"tinycolor2": "^1.6.0",
		"zod": "^3.22.4"
	}
}
